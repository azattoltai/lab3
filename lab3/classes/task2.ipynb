    "class Shape:\n",
    "    def area(self):\n",
    "        return 0  \n",
    "\n",
    "class Square(Shape):\n",
    "    def __init__(self, side_length):\n",
    "        self.side_length = side_length\n",
    "\n",
    "    def area(self):\n",
    "        return self.side_length ** 2  \n",
    "\n",
    "\n",
    "square = Square(4)\n",
    "print(\"Площадь квадрата:\", square.area())\n"
   ]
